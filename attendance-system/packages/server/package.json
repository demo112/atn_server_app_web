{
  "name": "@attendance/server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server/src/index.js",
  "scripts": {
    "build": "tsc",
    "test": "vitest --run src --exclude **/dist/**",
    "test:pbt": "vitest run \"**/*.pbt.test.ts\"",
    "test:pbt:quick": "vitest run \"**/*.pbt.test.ts\" --mode quick",
    "test:pbt:full": "vitest run \"**/*.pbt.test.ts\" --mode full",
    "test:watch": "vitest src --exclude **/dist/**",
    "lint": "eslint src",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --transpile-only --respawn src/index.ts",
    "start": "node dist/server/src/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@attendance/shared": "workspace:*",
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.67.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.0",
    "exceljs": "^4.3.0",
    "express": "^4.18.0",
    "express-async-errors": "^3.1.1",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^9.5.0",
    "@stryker-mutator/typescript-checker": "^9.5.0",
    "@stryker-mutator/vitest-runner": "^9.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "fast-check": "^4.5.3",
    "globals": "^17.3.0",
    "prisma": "^5.0.0",
    "supertest": "^7.2.2",
    "tiny-invariant": "^1.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18",
    "vitest-mock-extended": "^3.1.0"
  }
}
